@model IEnumerable<WebApplication8.Models.ContactForm>


@{
    ViewData["Title"] = "FormData";
    Layout = "~/Views/Shared/_Layout2.cshtml";
    ViewData["heading"] = "Contact Form";
}

<p style="border:1px solid green ; border-radius:2px 2px 2px 2px;width:10vw;padding:0.5vw;background:#fff;text-align:center;display:inline-block">
    <a>@Html.ActionLink("Patners Form","PatnerwithUs","Admin",new{r=@ViewBag.Number})</a>
</p>

<p style="border:1px solid green ; border-radius:2px 2px 2px 2px;width:10vw;padding:0.5vw;background:#fff;text-align:center;display:inline-block">
    <a>@Html.ActionLink("Pop Up","PatnerwithUs","Admin",new{r=@ViewBag.Number})</a>
</p>

<table class="table" style="background:#fff">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.url)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RefNumber)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @{
            var N = 1;
        }
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @N
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Phone)
                </td>
                
                <td>
                    @Html.DisplayFor(modelItem => item.url)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RefNumber)
                </td>
                
                
            </tr>
            N++;
        }
    </tbody>
</table>
