<div>
    <h5 style="display:inline">Category : </h5>
    <select name="Pages" id="Pages" onchange="DropDownShowHide(event)" style="margin-left:1vw" class="select">
        <option value="Homepages">Home pages</option>
        <option value="Platform">Platform</option>
        <option value="Solution">Solution</option>
        <option value="Company">Company</option>
        <option value="Resource">Resource</option>
    </select>
    <select name="PlatformPages" id="PlatformPages" style="display:none" onchange="DropDownShowHide(event)" class="select">
