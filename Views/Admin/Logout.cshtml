            <div class="col-sm-3">
                <h6 style="font-weight:400">Snippet Preview</h6>                
            </div>
            <div class="col-sm-8">
                <div style="width:50vw;height:25vh; border:1px solid red; background:#fff; padding:1vw">
                    <p style="color:red;">www.hhtpuhufehuhhh/joi/.com</p>
                    <h5 style="color:blue;">@ViewBag.data.Index</h5>
                    <p>@ViewBag.dataMeta.IndexMeta</p>
                </div>
                <div style="margin-top:1.5vh">
                    <input id="Check_Index" type="checkbox" onclick="checkUncheck(event)" @(

                        ViewBag.Checked.IndexMeta
