        <option value="AutomatedChatBot">Automated ChatBot</option>
        <option value="ConversationalMarketing">Conversational Marketing</option>
        <option value="AutoSegregation">Auto-Segregation</option>
        <option value="AppMarketing">App Marketing</option>
    </select>
    <select name="Pages" id="SolutionPages" style="display:none" onchange="DropDownShowHide(event)" class="select">
        <option value="Byusecase">By use case</option>
        <option value="ByIndustry">By Industry</option>
    </select>
    <select name="Pages" id="SolutioncasePages" style="display:none" onchange="DropDownShowHide(event)" class="select">
        <option value="WhatsAppMarketing">WhatsApp Marketing</option>
        <option value="WhatsappCommerce">Whatsapp Commerce</option>
        <option value="WhatsappSupport">Whatsapp Support</option>
        <option value="WhatsappAuthentication">Whatsapp Authentication</option>
    </select>
    <select name="Pages" id="SolutionindustryPages" style="display:none" onchange="DropDownShowHide(event)" class="select">
        <option value="EcommerceRetail">Ecommerce Retail</option>
        <option value="Healthcare">Healthcare</option>
        <option value="EdTech">EdTech</option>
        <option value="BankingBFSI">BankingBFSI</option>
        <option value="Mediaentertainment">Media entertainment</option>
        <option value="FoodBeverage">Food Beverage</option>
        <option value="TravelHospitality">Travel Hospitality</option>
    </select>
    <select name="Pages" id="CompanyPages" style="display:none" onchange="DropDownShowHide(event)" class="select">
        <option value="AboutUs">About Us</option>
        <option value="WorkWithUs">Work With Us</option>
        <option value="PartnerWithUs">Partner With Us</option>
    </select>
    <select name="Pages" id="ResourcePages" style="display:none" onchange="DropDownShowHide(event)" class="select"   >
        <option value="Blog">Blog</option>
        <option value="ContactUs">Contact Us</option>
