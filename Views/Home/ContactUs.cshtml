
@{
    ViewData["Title"] = @ViewBag.Title;
    ViewData["Meta"] = @ViewBag.Meta;
    ViewData["Check"] = @ViewBag.Check;
    ViewData["Schema"] = @ViewBag.Schema;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- form -->
<section class="py-5">

    <div class="container">
        <div class="section_title px-md-0 px-3" data-aos="fade-up">

            <h2 class="">Let's Get In <span>Touch</span></h2>
            <p>
                We'd Love To Know Your Whatsapp Use-Case And Help<br>
                Your Business Grow Using Whatsapp!
            </p>

        </div>
        <div class="row pt-4">
            <div class="col-md-6 col-lg-6 col-sm-12 my-auto">
                <img src="~/image/Contact-us.webp" class="img-fluid" alt="contact-img">


            </div>
            <div class="col-md-6 col-lg-6 col-sm-12 px-md-5 px-4 pt-md-0 pt-4">
                <div class="text-center justify-content-center mx-auto">
                    <div class="form_box px-md-5 px-3 py-md-5 py-3  text-center">
                        <h3 class="text-center pt-md-0 pt-3">Fill The Form Below</span></h3>
                        @model ContactUs
                        <form action="~/ContactUs1/Contact" method="POST">
                            
                            <label for="inputsubject"></label>
                            <input type="text" name="Name" class="form-control" id="name" placeholder="Name">

                            <label for="inputsubject"></label>
                            <input type="email" name="email"  class="form-control" id="email" placeholder="Your Email id*">

                            <label for="phone"></label>
                            <input type="text" name="phone" class="form-control" id="phone" placeholder="Phone Number*">
                            <label for="inputsubject"></label>
                            <input type="text" name="message" class="form-control" id="messages"
                                   placeholder="Name of Organisation">
                            <label for="inputsubject"></label>
                            <input type="url" name="url" class="form-control" id="url" placeholder="Website URL">



                            <div class="mt-3">
                                <input type="submit" class="px-5 py-2 login_button" value="Submit">
                            </div>

                        </form>


                    </div>
                </div>

            </div>
        </div>




    </div>

</section>
