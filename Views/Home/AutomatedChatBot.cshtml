<section class="banner_section pt-0">
    <!-- hero bg -->
    <!-- <div class="hero_bg"> <img src="images/hero-bg.webp
    " alt="image" > </div> -->
    <!-- container start -->
    <div class="container pt-md-5 mt-md-5">
        <!-- row start -->
        <div class="row">
            <div class="col-lg-6 col-md-12" data-aos="fade-right" data-aos-duration="1500">
                <!-- banner text -->
                <div class="banner_text">

                    <span class="tagline">
                        Automated ChatBot
                    </span>


                    <h1>
                        Chatbot: Your Virtual Assistant,
                        <span>
                            Handling it All!
                        </span>
                    </h1>

                    <p class="py-2">
                        Boost Conversions, Increase Revenue, and Foster Customer
                        Loyalty with No Code Chatbots on WhatsApp

