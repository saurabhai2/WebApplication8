                            <button type="button" class=" btn px-5" data-toggle="modal" data-target="#exampleModal">
                                Request Demo
                            </button>
                        </div>
                    </form>
                </div>



                <!-- users -->

            </div>

            <!-- banner images start -->
            <div class="col-lg-6 col-md-6 pt-md-0 pt-3">
                <img src="~/image/automated-chatbot.webp" class="img-fluid" alt="automated-chatbot">

            </div>



        </div>
        <!-- row end -->
    </div>
    <!-- container end -->
</section>
